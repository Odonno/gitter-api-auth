<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
    <metadata>
        <id>gitter-api-auth</id>
        <version>0.2.0</version>
        <title>Gitter# Auth</title>
        <authors>David Bottiau</authors>
        <owners>David Bottiau</owners>
        <projectUrl>https://github.com/Odonno/gitter-api-auth</projectUrl>
        <iconUrl>https://avatars0.githubusercontent.com/u/5990364</iconUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Gitter# Auth provide you a way to authenticate user on Gitter API</description>
        <releaseNotes>Rename namespace to prevent conflicts with gitter-api-pcl</releaseNotes>
        <copyright>Copyright 2015</copyright>
        <tags>Gitter Api Auth</tags>
        <dependencies>
            <group targetFramework="net45">
                <dependency id="Newtonsoft.Json" />
                <dependency id="System.Net.Http" />
            </group>
            <group targetFramework="win81">
                <dependency id="Microsoft.Net.Http" />
            </group>
            <group targetFramework="wpa81">
                <dependency id="Microsoft.Net.Http" />
            </group>
        </dependencies>
    </metadata>
    <files>
        <file src=".\GitterAuth\GitterAuth.Windows81\bin\Release\GitterAuth.*.dll" target="lib\win81" />
        <file src=".\GitterAuth\GitterAuth.WindowsPhone81\bin\Release\GitterAuth.*.dll" target="lib\wpa81" />
        <file src=".\GitterAuth\GitterAuth.NetFramework\bin\Release\GitterAuth.*.dll" target="lib\net45" />
    </files>
</package>
